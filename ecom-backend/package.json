{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:migrate": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js --knexfile knexfile.ts migrate:latest",
    "db:rollback": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js --knexfile knexfile.ts migrate:rollback",
    "db:seed": "node --loader ts-node/esm ./node_modules/knex/bin/cli.js --knexfile knexfile.ts seed:run",
    "db:reset": "npm run db:rollback --all && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.12.0",
    "stripe": "^16.8.0",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.6",
    "@types/node": "^22.5.2",
    "@types/uuid": "^9.0.7",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "knex-types": "0.5.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.6.2"
  }
}
